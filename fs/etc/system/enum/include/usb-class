#
# Definitions for usb classes
#

all
	set(USB_CLASS_DEVICE, 00)
	set(USB_CLASS_AUDIO, 01)
		set(USB_AUDIO_PROTOCOL_CONTROL, 01)
		set(USB_AUDIO_PROTOCOL_STREAMING, 02)
		set(USB_AUDIO_PROTOCOL_MIDI_STREAMING, 03)
	set(USB_CLASS_COMMS, 02)
		set(USB_COMMS_SUBCLASS_DIRECT_LINE, 01)
		set(USB_COMMS_SUBCLASS_MODEM, 02)
		set(USB_COMMS_SUBCLASS_TELEPHONE, 03)
		set(USB_COMMS_SUBCLASS_MULTICHANNEL, 04)
		set(USB_COMMS_SUBCLASS_CAPI_CONTROL, 05)
		set(USB_COMMS_SUBCLASS_ETHERNET, 06)
		set(USB_COMMS_SUBCLASS_ATM, 07)
		set(USB_COMMS_SUBCLASS_WIRELESS_HANDSET, 08)
		set(USB_COMMS_SUBCLASS_DEVICE_MGMT, 09)
		set(USB_COMMS_SUBCLASS_MOBILE_DIRECT_LINE, 0a)
		set(USB_COMMS_SUBCLASS_OBEX, 0b)
		set(USB_COMMS_SUBCLASS_ETHERNET_EMULATION, 0c)
			set(USB_COMMS_PROTOCOL_ETHERNET_EMULATION, 07)
	set(USB_CLASS_HID, 03)
		set(USB_HID_SUBCLASS_NONE, 00)
			set(USB_HID_PROTOCOL_NONE, 00)
			set(USB_HID_PROTOCOL_KEYBOARD, 01)
			set(USB_HID_PROTOCOL_MOUSE, 02)
		set(USB_HID_SUBCLASS_BOOTINTERFACE, 01)			
	set(USB_CLASS_PHYSICAL, 05)
	set(USB_CLASS_IMAGING, 06)
		set(USB_IMAGING_SUBCLASS_STILL, 01)
			set(USB_IMAGING_STILL_PROTOCOL_PTP, 01)
	set(USB_CLASS_PRINTER, 07)
	set(USB_CLASS_MASS_STORAGE, 08)
	set(USB_CLASS_HUB, 09)
		set(USB_HUB_PROTOCOL_FULL, 00)
		set(USB_HUB_PROTOCOL_HI_SINGLE_TT, 01)
		set(USB_HUB_PROTOCOL_HI_MULTIPLE_TT, 02)
	set(USB_CLASS_CDC_DATA, 0a)
	set(USB_CLASS_SMART_CARD, 0b)
	set(USB_CLASS_CONTENT_SECURITY, 0d)
	set(USB_CLASS_VIDEO, 0e)
	set(USB_CLASS_DIAGNOSTIC, dc)
	set(USB_CLASS_WIRELESS, e0)
		set(USB_WIRELESS_SUBCLASS_RADIO,01)
			set(USB_WIRELESS_PROTOCOL_BLUETOOTH_PI, 01)
			set(USB_WIRELESS_PROTOCOL_UWB_RCI, 02)
			set(USB_WIRELESS_PROTOCOL_RNDIS, 03)
		set(USB_WIRELESS_SUBCLASS_ADAPTER,02)
			set(USB_WIRELESS_PROTOCOL_HOST_WIRE, 01)
			set(USB_WIRELESS_PROTOCOL_DEVICE_WIRE, 02)
			set(USB_WIRELESS_PROTOCOL_DEVICE_WIRE_ISOCHRONOUS, 03)
	set(USB_CLASS_MISC, ef)
	set(USB_CLASS_APPSPEC, ef)
		set(USB_APPSPEC_SUBCLASS_FIRMWARE_UPDATE, 01)
			set(USB_APPSPEC_PROTOCOL_FIRMWARE_UPDATE, 01)
		set(USB_APPSPEC_SUBCLASS_IRDA_BRIDGE, 02)
			set(USB_APPSPEC_PROTOCOL_IRDA_BRIDGE, 00)
		set(USB_APPSPEC_SUBCLASS_TEST_MEASURE, 03)
			set(USB_APPSPEC_PROTOCOL_TMC, 00)
			set(USB_APPSPEC_PROTOCOL_USB488, 01)
	set(USB_CLASS_VENDOR_SPECIFIC, ff)
